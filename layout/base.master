					}
				.menu li a {
					color: #7c7c7c;
				}
					.menu li:hover {
						text-decoration: none;
						color: #2980b9;
					}
						.menu li:hover a {
							color: #2980b9;
							text-decoration: none;
						}
			.menu-icon {
				font-size: 16px;
				width: 22px;
			}
			.menu-text {
				padding-top: 5px;
				font-size: 11px;
			}
