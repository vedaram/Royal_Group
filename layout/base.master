        <!-- normalize.css -->
        <link rel="stylesheet" href=<%= ResolveUrl("~/resources/css/normalize.css") %>>
        <!-- critical CSS -->
        <style type="text/css">
            * {
                box-sizing: border-box;
            }
            html,
            body {
                height: 100%;
            }
            body {
            	background: #fcfcfc;
                color: #4d4d4d;
                font-size: 13px;
                font-weight: 300;
                font-family: arial, sans-serif;
                letter-spacing: 0.5px;
                padding: 0;
                margin: 0;
                overflow: hidden;
            }
            small {
                font-size: 11px !important;
            }
            a {
                color: #3498db;
                text-decoration: none;
            }
            a:hover {
                text-decoration: underline;
            }
            h1, h2, h3, h4, h5, h6 {
                padding: 0;
                margin: 0;
            }
            p {
                margin: 0;
                padding: 0;
            }
            ul {
            	list-style-type: none;
            	padding: 0;
            	margin: 0;
            }
            .pull-left {
            	float: left;
            }
            .pull-right {
            	float: right;
            }

            .hide {
            	display: none!important;
            }

            .container::before,
            .container::after {
            	display: table;
            	content: '';
            }
            .container::after {
            	clear: both;
            }

            /* sidebar */
            #saxPageSidebar {
            	position: absolute;
            	top: 90px;
            	bottom: 0px;
            	left: 0px;
            	width: 160px;
            	padding: 0px 15px;
            	border-top: 1px solid #eee;
            }
            	.brand {
            		padding: 3px 0; 
            		height: 35px;
            	}
				
				.brand img
              {
               width:80px;
               height:30px;
              }
            		.logo {
            			display: block;
            			width: 80px;
            			margin: 0 auto;
            		}

    		/* header */
    		#saxPageHeader {
    			background: #2980b9;
    			border-bottom: 1px solid #2980b9;
    			padding: 0 15px;
    			height: 35px;
    			font-size: 12px;
    		}
    			.header-menu {
    				line-height: 35px;
    				color: #fff;
    				font-size: 12px;
    			}
    				.header-menu li {
    					float: right;
    					margin-left: 20px;
    				}
    					.header-menu li a {
    						color: #fff;
    					}
			.profile-pic {
				width: 26px;
				line-height: 30px;
				vertical-align: middle;
			}
			#username {
				padding-left: 5px;
			}

			#saxPageTitleBar {
				position: absolute;
				top: 35px;
				left: 0px;
				right: 0px;
				height: 55px;
				border-top: 1px solid #fff;
				border-bottom: 1px solid #ddd;
				border-left: 1px solid #fff;
				background: #fafafa;
				padding: 5px 15px;
			}	
				.current-page-outer {
					margin: 10px 0;
				}
					.current-section {
						letter-spacing: 1.0px;
						padding-bottom: 5px;
					}
					.current-page {
						font-size: 20px;
						font-weight: bold;
					}

			#saxPageContent {
				position: absolute;
				top: 90px;
				bottom: 0px;
				left: 160px;
				right: 0px;
				border-top: 1px solid #eee;
				overflow-x: hidden;
				overflow-y: auto;
			}
				#saxPageContent .content {
					padding: 40px 15px 0;
				}
       	</style>
